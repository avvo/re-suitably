{
  "name": "@avvo/re-suitably",
  "version": "0.2.4",
  "description": "Avvo UI Kit React component library",
  "author": "Avvo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/avvo/re-suitably.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "link:example": "npm link && cd example && npm link re-suitably",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "lint:css": "stylelint './src/**/*.js'",
    "test": "jest test --env=jsdom --colors",
    "test:update": "yarn test -u",
    "clean:dist": "rm -rf ./dist",
    "build": "yarn clean:dist && rollup -c",
    "start": "yarn clean:dist && rollup -c -w",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "storybook": "start-storybook -p 9009",
    "storybook:publish": "build-storybook -c .storybook -o .out",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "@babel/preset-stage-3": "^7.0.0-beta.42",
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-regular": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-backgrounds": "^3.3.15",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-knobs": "^3.4.0-rc.3",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addon-storyshots": "^3.3.15",
    "@storybook/addon-viewport": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-styled-components": "^1.5.1",
    "dotenv-webpack": "^1.5.5",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "prop-types": "^15.5.4",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.4.0",
    "rollup-plugin-url": "^1.3.0",
    "storybook-addon-jest": "^0.0.1",
    "storybook-addon-jsx": "^5.3.0",
    "styled-components": "^3.2.3",
    "stylelint": "^9.1.3",
